    "@types/cache-manager": "^3.4.2",
    "@types/express": "^5.0.0",
    "@types/jest": "^30.0.0",
    "@types/morgan": "^1.9.3",
    "@types/multer": "^1.4.7",
    "@types/node": "^22.13.10",
    "@typescript-eslint/eslint-plugin": "8.26.1",
    "@typescript-eslint/parser": "8.26.1",
    "eslint": "9.22.0",
    "eslint-config-prettier": "10.0.1",
