    "@nestjs/platform-express": "^11.0.11",
    "@nestjs/schedule": "^5.0.1",
    "@nestjs/swagger": "^11.0.6",
    "app-root-path": "^3.0.0",
    "bullmq": "^5.43.1",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "dotenv": "^14.1.0",
    "envalid": "^8.0.0",
    "ioredis": "^5.6.0",
    "mongodb": "^6.18.0",
